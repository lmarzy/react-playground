{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "lib": ["es5", "es6", "dom"],
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true
  },
  "include": ["./src/**/*"]
}
